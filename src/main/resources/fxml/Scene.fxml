<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="323.0" prefWidth="497.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <Accordion fx:id="accordion" layoutX="22.0" layoutY="14.0" prefHeight="276.0" prefWidth="453.0">
        <panes>
          <TitledPane animated="false" prefHeight="93.0" prefWidth="453.0" text="Dane treningowe">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="browseButton" layoutX="164.0" layoutY="60.0" mnemonicParsing="false" onAction="#onBrowseButtonClick" text="Przeglądaj" />
                        <Label fx:id="trainingStatusLabel" layoutX="14.0" layoutY="123.0" text="Nie wczytano danych!" />
                     </children>
                  </AnchorPane>
            </content>
          </TitledPane>
          <TitledPane animated="false" text="Dane testowe">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <RadioButton fx:id="testRandomRadio" layoutX="14.0" layoutY="18.0" mnemonicParsing="false" text="Losuj z danych treningowych" />
                        <Button fx:id="testOptionsButton" layoutX="244.0" layoutY="14.0" mnemonicParsing="false" onAction="#onRandomOptionsButtonClick" text="Opcje" />
                        <RadioButton fx:id="testDiskRadio" layoutX="14.0" layoutY="48.0" mnemonicParsing="false" text="Z dysku" />
                        <Button fx:id="testBrowseButton" layoutX="112.0" layoutY="44.0" mnemonicParsing="false" onAction="#onTestBrowseButtonClick" text="Przeglądaj" />
                        <Button fx:id="testApplyButton" layoutX="161.0" layoutY="88.0" mnemonicParsing="false" onAction="#onTestApplyButtonClick" text="Dalej &gt;&gt;" />
