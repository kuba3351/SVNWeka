                     </children>
                  </AnchorPane>
            </content>
          </TitledPane>
          <TitledPane animated="false" text="Wyświetlanie i edycja danych">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button layoutX="96.0" layoutY="14.0" mnemonicParsing="false" onAction="#onTrainDataViewClick" text="Zobacz i edytuj dane treningowe" />
                        <Button layoutX="96.0" layoutY="66.0" mnemonicParsing="false" onAction="#onTestDataViewButtonClick" text="Zobacz i edytuj dane testowe" />
                        <Button layoutX="171.0" layoutY="105.0" mnemonicParsing="false" onAction="#onNextButtonClick" text="Dalej &gt;&gt;" />
                     </children>
                  </AnchorPane>
            </content>
          </TitledPane>
            <TitledPane animated="false" layoutX="10.0" layoutY="62.0" text="Klasyfikator SVN">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button layoutX="114.0" layoutY="17.0" mnemonicParsing="false" onAction="#onClassifierOptionsButtonClick" text="Ustaw opcje klasyfikatora" />
                        <Button layoutX="122.0" layoutY="59.0" mnemonicParsing="false" onAction="#onRunClassiferButtonClick" text="Uruchom klasyfikator" />
                     </children>
                  </AnchorPane>
               </content>
            </TitledPane>
            <TitledPane animated="false" layoutX="10.0" layoutY="88.0" text="Wynik">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button layoutX="114.0" layoutY="27.0" mnemonicParsing="false" onAction="#onResultButtonClick" text="Podgląd i zapis wyniku" />
                     </children>
                  </AnchorPane>
               </content>
            </TitledPane>
        </panes>
      </Accordion>
    </children>
</AnchorPane>
