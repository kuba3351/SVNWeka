<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="234.0" prefWidth="318.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Slider fx:id="percentSlider" layoutX="51.0" layoutY="43.0" />
      <Label layoutX="14.0" layoutY="14.0" text="Procent danych treningowych:" />
      <Label layoutX="14.0" layoutY="43.0" text="1" />
      <Label layoutX="205.0" layoutY="43.0" text="99" />
      <CheckBox fx:id="seedCheckBox" layoutX="14.0" layoutY="117.0" mnemonicParsing="false" onAction="#onSeedCheckBoxChange" text="Użyj własnego ziarna losowego" />
      <Label layoutX="14.0" layoutY="151.0" text="Ziarno losowe:" />
      <TextField fx:id="seedField" layoutX="120.0" layoutY="146.0" />
      <Button layoutX="72.0" layoutY="188.0" mnemonicParsing="false" onAction="#onOkButtonClick" text="Ok" />
      <Button layoutX="175.0" layoutY="188.0" mnemonicParsing="false" onAction="#onCancelButtonClick" text="Anuluj" />
      <Label fx:id="percentLabel" layoutX="102.0" layoutY="67.0" text="Label" />
   </children>
</AnchorPane>
